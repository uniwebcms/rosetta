{
  "name": "@uniwebcms/rosetta",
  "version": "0.1.0",
  "description": "A content parsing system that bridges natural markdown writing with component-based web development",
  "main": "lib/index.js",
  "type": "module",
  "bin": {
    "rosetta": "./lib/cli.js"
  },
  "scripts": {
    "build": "babel src -d lib",
    "test": "npm run build && jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "dev": "babel src -d lib --watch",
    "prepare": "npm run build",
    "pretest": "npm run build"
  },
  "keywords": [
    "markdown",
    "parser",
    "cms",
    "content",
    "components",
    "static-site",
    "uniweb"
  ],
  "author": "Proximify Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uniwebcms/rosetta.git"
  },
  "bugs": {
    "url": "https://github.com/uniwebcms/rosetta/issues"
  },
  "homepage": "https://github.com/uniwebcms/rosetta#readme",
  "dependencies": {
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "gray-matter": "^4.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "babel-jest": "^29.7.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
